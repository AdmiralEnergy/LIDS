# Twenty CRM Environment Variables
# Copy to .env and fill in values

# Database
POSTGRES_PASSWORD=your_secure_password_here

# URLs (use your domain or IP)
SERVER_URL=https://twenty.yourdomain.com
FRONT_BASE_URL=https://twenty.yourdomain.com

# Secrets (generate with: openssl rand -base64 32)
APP_SECRET=generate_random_base64_string
ACCESS_TOKEN_SECRET=generate_random_base64_string
LOGIN_TOKEN_SECRET=generate_random_base64_string
REFRESH_TOKEN_SECRET=generate_random_base64_string
